def vowels(userInput):
    listVowels = ['A','a','E','e','I','i','O','o','U','u']
    i = 0
    total = 0
    while i < len(listVowels):
        mod = userInput.count(listVowels[i])
        total += mod
        i += 1
    print(mod)


        
    
if __name__ == '__main__':
    userInput = input('Enter a statement:\n')
    vowels(userInput)
